# Copy to fastlane/.env and fill with your credentials before running Fastlane commands.
# All values are optional unless noted; leave blank if not applicable.

# Apple Developer Account
APPLE_ID=
TEAM_ID=
ITC_TEAM_ID=

# Match (Code Signing)
MATCH_GIT_URL=
MATCH_GIT_BRANCH=main
MATCH_PASSWORD=

# App Store Connect API
APP_STORE_CONNECT_API_KEY_ID=
APP_STORE_CONNECT_ISSUER_ID=
# Base64 encoded private key content (use APP_STORE_CONNECT_API_KEY_PATH or JSON alternative instead)
APP_STORE_CONNECT_API_KEY_CONTENT=

# TestFlight Configuration
TESTFLIGHT_GROUPS=Internal Testers

# Optional release notes
CHANGELOG=
