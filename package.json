{
  "name": "sept122025",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prebuild": "expo prebuild -p all",
    "android:release": "expo run:android --variant release",
    "android:release:build-only": "expo run:android --variant release --no-install",
    "ios:release": "RCT_NO_LAUNCH_PACKAGER=1 expo run:ios --configuration Release --device \"$IOS_DEVICE\"",
    "ios:release:build-only": "RCT_NO_LAUNCH_PACKAGER=1 expo run:ios --configuration Release --no-install",
    "ios:devices": "xcrun xctrace list devices || true",
    "screenshots": "node scripts/capture-screenshots.mjs",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\" \"components/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.{js,jsx,ts,tsx,mts,mjs}\"",
    "format:check": "prettier --check \"app/**/*.{js,jsx,ts,tsx}\" \"components/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.{js,jsx,ts,tsx,mts,mjs}\"",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package",
    "testflight": "bash -lc 'set -a && source .env && bundle exec fastlane deploy'"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.8",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.8",
    "expo-file-system": "~19.0.13",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.2",
    "expo-splash-screen": "~31.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "pnpm": {
    "overrides": {
      "glob": "^10.4.5",
      "rimraf": "^5.0.7",
      "inflight": "npm:@npmcli/inflight@^2.0.0"
    }
  }
}
