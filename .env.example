# Copy this file to .env and fill in the values before running fastlane lanes

# Apple Developer account
APPLE_ID=
FASTLANE_USER=$APPLE_ID
APPLE_TEAM_ID=
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=
APP_NAME=
APP_SKU=
APP_PRIMARY_LANGUAGE=English

# Fastlane Match configuration
MATCH_GIT_URL=
MATCH_GIT_BRANCH=main
MATCH_PASSWORD=

# App Store Connect API key (base64 encoded .p8 content preferred; raw PEM also works)
APP_STORE_CONNECT_API_KEY_ID=
APP_STORE_CONNECT_ISSUER_ID=
APP_STORE_CONNECT_API_KEY_CONTENT=

# Optional flags
SKIP_WAITING=false
DISTRIBUTE_EXTERNAL=false
CHANGELOG=
